.my-login {
  height: 350rpx;
  background: rgb(94,198,11);
  display: flex;
  justify-content: center;
  align-items: center;
}
.button-login {
  height: 25%;
  width: 50%;
  background: rgb(255,255,255);
  color: rgb(32,69,38);
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50rpx;
}
.head-message {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.head-portrait {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  margin-bottom: 10rpx
}

.my-body {

  padding: 20rpx;
  display: flex;
  flex-direction: column;
}
.my-body-title {
  font-size: 105%;
  font-weight: 700;
}